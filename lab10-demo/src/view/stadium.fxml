<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment=""  fx:controller="controller.StadiumController" xmlns:fx="http://javafx.com/fxml" prefHeight="400" prefWidth="400" styleClass="root">
    <ListView fx:id="groupsLv" items="${controller.stadium.groups}" prefHeight="300" prefWidth="300"/>


    <HBox alignment="CENTER" >
        <Button wrapText="true"  fx:id="openBtn" text="Open" onAction="#handleOpen"/>
    </HBox>
    <stylesheets>
        <URL value="@stadium.css"/>
    </stylesheets>    
</VBox>
